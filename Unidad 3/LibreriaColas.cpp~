#include <iostream>
#include <queue>
#include <deque>

using namespace std;

int main()
{
	//Colas Simple
	cout << "\nColas simples: ";
	queue<int> p;

	p.push(10); p.push(20); p.push(30);

	while(!p.empty())
	{
		cout << p.front();
		if(p.size()>1) cout << " , ";
		p.pop();
	}


	//Bicolas
	cout << "\nBicolas";
	deque<int> q;
	
	q.push_front(10); q.push_front(20);
	q.push_back(30); q.push_back(40);

	for(int a : q) //for each 
	{
		cout << a << endl;
	}
	return 0;
}
